
public class fibbonacci {

	public static void fibonacci() {
		int a = 0;
		int b = 1;
		int c = -1;
		System.out.print(a + " " + b + " ");
		for (int i = 1; i <= 18; i++) {
			c = a + b;
			System.out.print(c + " ");
			a = b;
			b = c;
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		fibonacci();
	}

}
